{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"oneOf": [
		{
			"$ref": "#/definitions/noProp"
		},
		{
			"$ref": "#/definitions/compoundProp"
		},
		{
			"$ref": "#/definitions/listProp"
		},
		{
			"properties": {
				"ref": {
					"type": "string"
				}
			},
			"required": ["ref"]
		},
		{
			"properties": {
				"context": {
					"type": "object",
					"properties": {
						"ref_dict": {
							"type": "array",
							"items": {
								"type": "object",
								"properties": {
									"name": {
										"type": "string"
									},
									"nbt_ref": {
										"type": "string"
									}
								}
							}
						},
						"ref": {
							"type": "string"
						},
						"default": {
							"type": "string"
						}
					},
					"required": [
						"ref_dict",
						"ref",
						"default"
					]
				}
			},
			"required": ["context"]
		}
	],
	"definitions": {
		"uri": {
			"type": "string",
			"pattern": ""
		},
		"noProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"no-nbt",
						"byte",
						"short",
						"int",
						"long",
						"float",
						"double",
						"byte_array",
						"string",
						"int_array",
						"long_array"
					]
				}
			}
		},
		"compoundProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"root",
						"compound"
					]
				},
				"children": {
					"type": "object",
					"patternProperties": {
						".*": {
							"$ref": "#"
						}
					}
				},
				"child_ref": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			}
		},
		"listProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"list"
					]
				},
				"item": {
					"$ref": "#"
				}
			}
		}
	}
}