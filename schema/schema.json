{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"oneOf": [
		{
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		{
			"$ref": "#/definitions/nest_node"
		}
	],
	"definitions": {
		"nest_node": {
			"type": "object",
			"properties": {
				"suggestions": {
					"type": "array",
					"items": {
						"oneOf": [
							{
								"type": "string"
							},
							{
								"type": "object",
								"properties": {
									"value": {
										"type": "string"
									},
									"description": {
										"type": "string"
									}
								},
								"required": [
									"value",
									"description"
								]
							},
							{
								"type": "object",
								"properties": {
									"values": {
										"type": "string"
									}
								},
								"required": [
									"values"
								]
							}
						]
					}
				},
				"description": {
					"type": "string"
				}
			},
			"oneOf": [	
				{
					"$ref": "#/definitions/noProp"
				},
				{
					"$ref": "#/definitions/compoundProp"
				},
				{
					"$ref": "#/definitions/listProp"
				},
				{
					"properties": {
						"ref": {
							"type": "string"
						}
					},
					"required": ["ref"]
				},
				{
					"properties": {
						"context": {
							"type": "object",
							"properties": {
								"ref_dict": {
									"type": "array",
									"items": {
										"properties": {
											"name": {
												"type": "string"
											}
										},
										"required": [
											"name"
										],
										"oneOf": [
											{
												"type": "object",
												"properties": {
													"nbt_ref": {
														"type": "string"
													}
												},
												"required": [
													"nbt_ref"
												]
											},
											{
												"type": "object",
												"properties": {
													"info_name": {
														"type": "string",
														"enum": [
															"type", "id"
														]
													}
												},
												"required": [
													"info_name"
												]
											}
										]
									}
								},
								"ref": {
									"type": "string"
								},
								"default": {
									"type": "string"
								}
							},
							"required": [
								"ref_dict",
								"ref",
								"default"
							]
						}
					},
					"required": ["context"]
				}
			]
		},
		"noProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"no-nbt",
						"byte",
						"short",
						"int",
						"long",
						"float",
						"double",
						"byte_array",
						"string",
						"int_array",
						"long_array"
					]
				}
			}
		},
		"compoundProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"root",
						"compound"
					]
				},
				"children": {
					"type": "object",
					"patternProperties": {
						".*": {
							"$ref": "#/definitions/nest_node"
						}
					}
				},
				"child_ref": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			}
		},
		"listProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"list"
					]
				},
				"item": {
					"$ref": "#/definitions/nest_node"
				}
			}
		}
	}
}