{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"oneOf": [
		{
			"type": "array",
			"items": {
				"oneOf": [
					{
						"type": "string"
					},
					{
						"type": "object",
						"properties": {
							"value": {
								"type": "string"
							},
							"description": {
								"type": "string"
							}
						},
						"required": [
							"value",
							"description"
						]
					}
				]
			}
		},
		{
			"$ref": "#/definitions/nest_node"
		}
	],
	"definitions": {
		"nest_node": {
			"type": "object",
			"properties": {
				"suggestions": {
					"type": "array",
					"items": {
						"oneOf": [
							{
								"type": "string"
							},
							{
								"type": "object",
								"properties": {
									"value": {
										"type": "string"
									},
									"description": {
										"type": "string"
									}
								},
								"required": [
									"value",
									"description"
								]
							},
							{
								"type": "object",
								"properties": {
									"values": {
										"type": "string"
									}
								},
								"required": [
									"values"
								]
							}
						]
					}
				},
				"description": {
					"type": "string"
				}
			},
			"oneOf": [	
				{
					"$ref": "#/definitions/noProp"
				},
				{
					"$ref": "#/definitions/compoundProp"
				},
				{
					"$ref": "#/definitions/listProp"
				},
				{
					"properties": {
						"ref": {
							"type": "string"
						}
					},
					"required": ["ref"]
				},
				{
					"properties": {
						"function": {
							"type": "object",
							"properties": {
								"id": {
									"type": "string"
								},
								"params": {
									"type": "object"
								}
							},
							"required": [
								"id"
							]
						}
					},
					"required": ["function"]
				}
			]
		},
		"noProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"no-nbt",
						"byte",
						"short",
						"int",
						"long",
						"float",
						"double",
						"byte_array",
						"string",
						"int_array",
						"long_array"
					]
				}
			}
		},
		"compoundProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"root",
						"compound"
					]
				},
				"children": {
					"type": "object",
					"patternProperties": {
						".*": {
							"$ref": "#/definitions/nest_node"
						}
					}
				},
				"child_ref": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			}
		},
		"listProp": {
			"required": [
				"type"
			],
			"properties": {
				"type": {
					"enum": [
						"list"
					]
				},
				"item": {
					"$ref": "#/definitions/nest_node"
				}
			}
		}
	}
}